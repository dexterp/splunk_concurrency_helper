<form>
  <init>
    <set token="idx">idx*</set>
    <set token="sh">sh*</set>
    <set token="cm">c0m1*</set>
  </init>
  <label>Summary S3 activity</label>
  <search id="cache_access">
    <query> 
    index=_internal sourcetype::splunkd_access  TERM(cacheman) (TERM(dma) OR TERM(ra)) 
| rex field=_raw "HTTP/1.1\"\s+(?&lt;status&gt;\S+)"
| rex field=uri ".services.admin.cacheman.(?&lt;accel_type&gt;(?:ra|dma))\|(?&lt;bucketid&gt;\S+?)\|(?&lt;SH_GUID&gt;\S+?)_(?&lt;acceleration&gt;\S+?)\?output_mode=json&amp;sid=remote_(?&lt;search_head&gt;\S+?)_(?&lt;GUID&gt;\S+?)$$"
|rex mode=sed field=acceleration "s/\/close//"
|rex mode=sed field=GUID "s/\&amp;.*//"
|bin _time span=$span$
|stats count by status accel_type SH_GUID acceleration search_head GUID _time
</query>
    <earliest>$timerange.earliest$</earliest>
    <latest>$timerange.latest$</latest>
  </search>
  <fieldset submitButton="true" autoRun="false">
    <input type="time" token="timerange" searchWhenChanged="false">
      <label></label>
      <default>
        <earliest>-4h@m</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="text" token="span">
      <label>span</label>
      <default>5m</default>
      <initialValue>5m</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>High level Overview</title>
      <table>
        <search>
          <query>(index=_internal host=$idx$ sourcetype=splunkd
(component=S3Client "ERROR" "Slow Down") OR (Forcing a re-add, Reason="Proxy is in disconnect state.") OR (statusDescription="Error resolving: Name or service not known"))
OR
(index=_internal source=*metrics* host=$idx$ (group=cachemgr_download OR group=cachemgr_upload OR group=spacemgr))
|eval cache_uploads=if(group="cachemgr_upload",succeeded,0)
|eval cache_downloads=if(group="cachemgr_download",succeeded,0)
|eval slow_down=if(statusDescription="Slow Down", 1, 0)
|eval host_re_add=if(Reason="Proxy is in disconnect state.", 1, 0)
|eval dns_errors=if(statusDescription="Error resolving: Name or service not known", 1, 0)
|stats sum(slow_down) as slow_down sum(host_re_add) as host_re_add sum(dns_errors) as dns_errors sum(cache_uploads) as cache_uploads sum(cache_downloads) as cache_downloads
|sort - dns_errors</query>
          <earliest>$timerange.earliest$</earliest>
          <latest>$timerange.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
