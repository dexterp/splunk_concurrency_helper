<form>
  <label>Bundle Push Investigator _internal</label>
  <fieldset submitButton="true" autoRun="false">
    <input type="time" token="time" searchWhenChanged="true">
      <label>Set a time range</label>
      <change>
        <condition value="*">
          <set token="time_picker">true</set>
        </condition>
      </change>
      <default>
        <earliest>-4h@m</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Bundle push REST invocation</title>
      <table>
        <search>
          <done>
            <set token="operation_count">$job.resultCount$</set>
          </done>
          <query>index=_internal host=c0m1*
sourcetype=splunkd_access
((control  (TERM(validate_bundle) OR apply OR TERM(cancel_bundle_push) OR rollback))
OR
clusterbundles )
|reverse
| streamstats count(eval(match(uri,"control"))) AS sessionID 
| streamstats  count   range(_time) as duration median(spent) as medianbundlespent sum(spent) as totalbundlespent avg(bytes) as bundlesize  reset_before="("match(uri,\"master/control\")")"
|stats earliest(file) as start_action latest(file) as end_action max(count) as count earliest(_time) as _time latest(medianbundlespent) as medianbundlespent latest(totalbundlespent) as totalbundlespent latest(duration) as duration latest(bundlesize) as bundlesize by sessionID 
|eval bundlesizeMB=round(bundlesize/1024/1024,0)
|fields - bundlesize
|eval peers_downloaded=count-1
|fields - count sessionID</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="count">20</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="number" field="bundlesizeMB"></format>
        <format type="color" field="bundlesizeMB">
          <colorPalette type="minMidMax" maxColor="#D93F3C" midColor="#ED8440" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax" maxValue="100" midValue="10" minValue="0"></scale>
        </format>
        <drilldown>
          <condition match="NOT match('row.op_type', &quot;upgrade&quot;)">
            <set token="op_uuid">$row.op_uuid$</set>
            <set token="stack">$row.stack$</set>
            <set token="op_start_time">$row.start time$</set>
            <set token="op_end_time">$row.end time$</set>
            <set token="op_type_not_upgrade">yes</set>
          </condition>
          <condition match="1 = 1">
            <set token="op_uuid">$row.op_uuid$</set>
            <set token="stack">$row.stack$</set>
            <set token="op_start_time">$row.start time$</set>
            <set token="op_end_time">$row.end time$</set>
            <unset token="op_type_not_upgrade"></unset>
          </condition>
        </drilldown>
      </table>
    </panel>
  </row>
  <row>
